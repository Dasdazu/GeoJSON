<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
    <Document>
        <name>Perth Polygon Style</name>
        <Style id="polygonStyle">
            <LineStyle>
                <color>ff0000ff</color> <!-- Blue color -->
                <width>5</width> <!-- Stroke weight -->
            </LineStyle>
            <PolyStyle>
                <color>FF697900</color> <!-- Turquoise color with 50% opacity "80" for 50% opacity in the alpha location-->
            </PolyStyle>
        </Style>
    </Document>
</kml>
